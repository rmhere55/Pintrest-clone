<body>
  <div class="header">
    <div class="header-logo">
      <i class="fa-brands fa-pinterest fa-xl" style="color: #b8281e;"></i>
    </div>
    <div class="header-nav">
      <a href="#">Home</a>
      <a href="#">Explore</a>
      <a href="#">Create</a>
    </div>
    <div class="header-search">
      <input type="text" placeholder="Search">
      <i class="search-icon fa fa-search"></i>
    </div>
    <div class="header-notifications">
      <div class="notification">
        <i class="fa fa-bell"></i>
        <span class="notification-count">99+</span>
      </div>
      <div class="notification">
        <i class="fa fa-comment"></i>
      </div>
    </div>
    <div class="header-user" >
      <img src="https://i.pinimg.com/originals/65/42/e2/6542e2410f65f53e85b5f8d4c21a4e48.png" alt="User Profile Picture">
      <div class="user-dropdown">
        <a href="#">Profile</a>
        <a href="#">Settings</a>
        <a href="#">Logout</a>
      </div>
    </div>
  </div>

